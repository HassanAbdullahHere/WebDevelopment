      /* CSS for <li> tag */
.nav li {
  display: inline-block;
  margin-right: 15px;
  position: relative;
  transition: transform 0.3s ease-in-out;
}

/* Animation */
.nav li::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  width: 0;
  height: 2px;
  background-color: #712cf9;
  transition: width 0.3s ease-in-out;
}

/* Hover effect */
.nav li:hover {
  transform: translateY(-2px);
}

.nav li:hover::after {
  width: 100%;
}

/* Active page styling */
.nav li.active::after {
  width: 100%;
}

/* Text styling */
.nav li a {
  text-decoration: none;
  color: #fff;
  text-transform: uppercase;
  font-family: "Arial", sans-serif;
  font-weight: bold;
  letter-spacing: 1px;
}

/* Hover effect on text */
.nav li a:hover {
  color: lightblue;
}

